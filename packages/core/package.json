{
	"name": "@omnidev-ai/core",
	"version": "0.8.0",
	"type": "module",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/Nikola-Milovic/omnidev.git",
		"directory": "packages/core"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"bun": "./src/index.ts",
			"default": "./dist/index.js"
		},
		"./hooks": {
			"types": "./dist/hooks/index.d.ts",
			"bun": "./src/hooks/index.ts",
			"default": "./dist/hooks/index.js"
		},
		"./test-utils": {
			"types": "./dist/test-utils/index.d.ts",
			"bun": "./src/test-utils/index.ts",
			"default": "./dist/test-utils/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/test-utils/**",
		"src",
		"!src/test-utils/**",
		"!src/**/*.test.ts",
		"README.md"
	],
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	},
	"scripts": {
		"typecheck": "tsc --noEmit",
		"build": "bunup"
	},
	"dependencies": {
		"@stricli/core": "^1.2.5",
		"smol-toml": "^1.6.0"
	},
	"devDependencies": {
		"bunup": "^0.16.20"
	}
}
